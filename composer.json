{
    "name": "amylashley/traffic-logger",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "amylashley",
        "trafficlogger",
        "laravel logger"
    ],
    "homepage": "https://github.com/amylashley/traffic-logger",
    "license": "MIT",
    "authors": [
        {
            "name": ":Amy Lashley",
            "email": "alashley@amherst.edu"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "AmyLashley\\TrafficLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AmyLashley\\TrafficLogger\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmyLashley\\TrafficLogger\\TrafficLoggerServiceProvider"
            ]
        }
    }
}
